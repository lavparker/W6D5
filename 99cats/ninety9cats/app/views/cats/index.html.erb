<h1> Here are all of my cats: </h1>
<ul>
<% @cats.each do |cat| %>
    <li> <%= cat.name %> </li>
    <li> <%= cat.birth_date %> </li>
    <li> <%= cat.sex %> </li>
    <li> <%= cat.color %> </li>
    <li> <%= cat.age %> </li>
    <li> <%= cat.description %> </li>
    <li> 
        <a href="<%= cat_url(cat) %>">Cat's show page</a>
    </li>
    <br />
<% end %>

</ul>